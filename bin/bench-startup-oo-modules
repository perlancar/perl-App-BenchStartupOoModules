#!perl

use 5.010001;
use strict;
use warnings;

use Benchmark::Command;

Benchmark::Command::run(
    undef,
    {
        'none'                 => [$^X, "-e1"],
        'Mo'         => [$^X, "-e", "use Mo"],
        'Moo'        => [$^X, "-e", "use Moo"],
        'Moose'      => [$^X, "-e", "use Moose"],
        'Mouse'      => [$^X, "-e", "use Mouse"],
        'Mojo::Base' => [$^X, "-e", "use Mojo::Base"],
    },
);

# ABSTRACT: Benchmark startup time of various OO system modules
# PODNAME:

=head1 SYNOPSIS

 % bench-startup-oo-modules

Sample output:

# COMMAND: perl bin/bench-startup-oo-modules 2>&1
